{
  "expo": {
    "jsEngine": "hermes",
    "name": "GymGenius",
    "scheme": "GymGenius",
    "slug": "gymgenius",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gilm.gymgenius"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      [
        "expo-asset",
  
        {
          "assets": ["./assets/exercise_collection/*/*.gif"]
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
